[Unit]
Description=Runtime Initialization Service
After=network.target network-setup.service
Requires=network-setup.service

[Service]
Type=oneshot
ExecStart=/usr/bin/runtime-init
ExecStartPost=/usr/bin/tdx-init setup /etc/tdx-init/config.yaml
RemainAfterExit=yes
Restart=on-failure
RestartSec=5

[Install]
WantedBy=minimal.target
